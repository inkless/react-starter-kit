{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "function-name": false,
    "max-line-length": [true, 120],
    "ter-indent": [
      true,
      2,
      { "SwitchCase": 1 }
    ],
    "variable-name": false,
    "jsx-alignment": true,
    "jsx-equals-spacing": [true, "never"],
    "jsx-no-string-ref": true,
    "jsx-self-close": true,
    "jsx-wrap-multiline": true,
    "import-name": false,
    "react-hooks-nesting": "error"
  },
  "rulesDirectory": []
}
